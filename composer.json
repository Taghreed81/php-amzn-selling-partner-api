{
    "name": "jasara/php-amzn-selling-partner-api",
    "description": "A fluent interface for Amazon's Selling Partner API in PHP",
    "type": "library",
    "license": "Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International Public License",
    "authors": [
        {
            "name": "Jasara Technology Inc.",
            "email": "keith@jasaratech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jasara\\AmznSPA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasara\\AmznSPA\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "spatie/data-transfer-object": "dev-master",
        "ocramius/lazy-property": "2.3.x-dev",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "illuminate/http": "8.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "illuminate/support": "8.x-dev",
        "spatie/ray": "dev-main"
    }
}