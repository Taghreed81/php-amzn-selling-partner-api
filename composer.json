{
    "name": "jasara/php-amzn-selling-partner-api",
    "description": "A fluent interface for Amazon's Selling Partner API in PHP",
    "type": "library",
    "license": "Creative Commons Attribution-NonCommercial-ShareAlike 4.0 International Public License",
    "authors": [
        {
            "name": "Jasara Technology Inc.",
            "email": "keith@jasaratech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jasara\\AmznSPA\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jasara\\AmznSPA\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ocramius/lazy-property": "^2.3",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^8",
        "spatie/data-transfer-object": "^3.7",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "aws/aws-sdk-php": "dev-feature-guzzle-2.0",
        "phpunit/phpunit": "^9",
        "illuminate/support": "^8",
        "spatie/ray": "^1.30"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SamRemis/aws-sdk-php"
        }
    ]
}